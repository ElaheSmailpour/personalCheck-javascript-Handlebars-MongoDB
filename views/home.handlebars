<div>


  <header>
    <nav>
      <ul>
        <li>my profile
          <ul>
           
            <a href="/user/logout">
              <li>Logout</li>
            </a>
          </ul>
        </li>
      </ul>
    </nav>
    <div>
      <h3>{{username}}</h3>
    </div>
    <button {{#if startWorkDisabled}} disabled {{/if}} onclick="startWork()">start Work</button>
    <button {{#if endWorkDisabled}} disabled {{/if}}  onclick="endWork()">End Work</button>
    <button {{#if pauseStartDisabled}} disabled {{/if}}  onclick="startPause()">Pause Start</button>
    <button {{#if pauseEndDisabled}} disabled {{/if}}onclick="endPause()">Pause End</button>
    
  </header>
  <div>
    <a href="/home?date={{lastMonthNumber}}"><button><</button></a>
    <a href="/home?date={{nextMonthNumber}}"><button>></button></a>
    <span>{{currentMonth}}</h6>
  </div>
  <div class="calender">
    <div class="dayHeader">
      <h3>mon</h3>
    </div>
     <div class="dayHeader">
      <h3>tue</h3>
    </div>
     <div class="dayHeader">
      <h3>wed</h3>
    </div>
     <div class="dayHeader">
      <h3>thu</h3>
    </div>
     <div class="dayHeader">
      <h3>fri</h3>
    </div>
     <div class="dayHeader">
      <h3>sat</h3>
    </div>
     <div class="dayHeader">
      <h3>sun</h3>
    </div>
    {{#each days}}
    <div class="dayCell">
      {{#if this.startTime}}
      <h3>employed</h3>
      {{/if}}
      <h5>
        {{this.dayWork}}
      </h5>
      {{#if this.startTime}}
    <span>{{this.startTime}}=====> {{this.endTime}}</span>
    <span>{{this.pauseDuration}}</span>
      {{/if}}
      </div>
  {{/each}}

  </div>

</div>
<script src="/Js/Homescript.js"></script>